<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Viento Sur{% endblock %}</title>
        <!-- GOOGLE FONTS -->
        <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,700' rel='stylesheet' type='text/css'>
        <!-- bootstrap  -->
        <link href="{{ asset('bundles/vientosurappapp/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
        <!-- font Awesome -->
        <link href="{{ asset('bundles/vientosurappapp/css/font-awesome.css') }}" rel="stylesheet" type="text/css" />
        <!-- --><!-- Ionicons -->
        <link rel="stylesheet" href="{{ asset('bundles/vientosurappapp/css/icomoon.css') }}">
        <!--  -->
        <script src="{{ asset('bundles/vientosurappapp/js/modernizr.js')}}" type="text/javascript"></script>
        <!--  -->
        <link href="{{ asset('bundles/vientosurappapp/css/styles.css') }}" rel="stylesheet" type="text/css" />
        <!--  -->
        <link href="{{ asset('bundles/vientosurappapp/css/vs-global.css') }}" rel="stylesheet" type="text/css" />
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="//code.jquery.com/jquery-1.10.2.js"></script>
        <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/modernizr.js') }}"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/bootstrap.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/slimmenu.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/bootstrap-datepicker.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/bootstrap-datepicker.es.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/vientosurappapp/js/bootstrap-timepicker.js') }}" type="text/javascript"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
