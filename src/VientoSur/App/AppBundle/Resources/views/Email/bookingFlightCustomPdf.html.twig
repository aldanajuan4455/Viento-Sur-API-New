                                            <h7 style="margin: 0; padding: 0; text-align: center;display: block;">{{ 'index.search.hotel.childs' | trans }}</h7>
                                            <h3 style="text-align: center;">{{ itineraryDetail.passenger_distribution.children }}</h3>
                                        </div>
                                        {% endif %}
                                        {% if itineraryDetail.passenger_distribution.infants > 0 %}
