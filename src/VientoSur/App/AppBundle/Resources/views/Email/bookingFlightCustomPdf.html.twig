                                    <p style="text-align: right;display: inline-block; float: right;">
                                        {% if itineraryDetail.outbound_choices[0].segments | length < 1 %}
                                            {{ 'flights.stop' | trans }} {{ itineraryDetail.outbound_choices[0].segments | length }}
                                        {% else %}
                                            {{ itineraryDetail.outbound_choices[0].segments | length -1}} {{ 'flights.stop' | trans }}s
                                        {% endif %}
                                    </p>
                                </div>

                                {% if itineraryDetail.outbound_choices[0].segments | length > 0 %}
