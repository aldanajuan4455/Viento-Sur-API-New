                                            <h7 style="margin: 0; padding: 0; text-align: center;display: block;">{{ 'flights.labels.infants' | trans }}</h7>
                                            <h3 style="text-align: center;">{{ itineraryDetail.passenger_distribution.infants }}</h3>
                                        </div>
                                        {% endif %}
                                    </td>
                                </tr>
                            </table>
                            {% if itineraryDetail.outbound_choices is defined %}
