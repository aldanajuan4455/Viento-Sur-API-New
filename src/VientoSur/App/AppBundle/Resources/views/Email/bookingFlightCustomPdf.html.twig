                                                </tr>
                                            </table>

                                            <table width="200" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row" style="margin-right: 10px;border-right: #eee 1px solid;">
                                                <tr>
                                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:center;color:#333333;width:100%">
                                                        {{ 'flights.outgoing' | trans }}
                                                        <h3 style="margin: 0;">{{ item.from }}</h3>
                                                        <h3 style="margin: 0;">{{ item.departure_datetime | localizeddate('long', 'none', app.request.locale) }}</h3>
                                                        <h3 style="margin: 0;">{{ item.departure_datetime | localizeddate('none', 'short', app.request.locale) }}</h3>
                                                    </td>
                                                </tr>
                                            </table>
                                            <br>
                                        {% endfor %}
                                    {% endif %}

                                {% endif %}
                            {% endif %}
                            <br>
                            <div class="hr" style="height:1px;border-bottom:1px solid #cccccc;clear: both;">&nbsp;</div>
                            {% if pdf is not defined  %}
                                <br>
                                <div class="subtitle" style="font-family:Helvetica, Arial, sans-serif;font-size:16px;font-weight:600;color:#FFFFFF">
                                    <a href="{{ url('viento_sur_app_booking_flight_pdf',{'itinerary':itineraryDetail.id, 'reservation': reservation.id}) }}" style="color:#ffffff;border:0;display: inline-block;padding: 14px 25px;opacity: 0.9;background-color: #008cd1;text-align: center;text-decoration: none;font-size: 16px;margin-left: 20px;"><i class="fa fa-download"></i> {{ 'hotels.download_pdf' | trans }}</a>
                                </div>
                            {% endif %}
                            <br>
                                <br>
                                <i class="fa fa-clock-o"></i> <em><small>{{ 'index.flight_remember' | trans }}</small></em>
                            </div>

                            <br>
                        </td>
                    </tr>
                </table>
