                                        <br>
                                        <table width="200" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row" style="margin-right: 10px;">
                                            <tr>
                                                <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333;width:100%">
                                                    <div style="width:100%;">
                                                        <strong>{{ 'flights.waiting_time' | trans }}</strong>:
                                                        {{ segment.duration|split(':')[0] }}h {{ segment.duration|split(':')[1] }}m
                                                    </div>
                                                    <div style="width:100%;">
                                                        <strong>{{ 'index.class' | trans }}</strong>:
                                                        {{ segment.cabin_type | capitalize }}
                                                    </div>
                                                </td>
                                            </tr>
                                        </table>
                                        <table width="200" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row" style="margin-right: 10px;">
