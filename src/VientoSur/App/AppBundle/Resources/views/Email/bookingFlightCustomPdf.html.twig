                                    <i class="fa fa-plane"></i>  <p style="text-align: left;display: inline-block;">{{ 'index.search.flight.stretch' | trans }} {{ loop.index }}</p>
                                    <p style="text-align: right;display: inline-block; float: right;">
                                        {% if item.segments | length < 1 %}
                                            {{ 'flights.stop' | trans }} {{ item.segments | length }}
                                        {% else %}
                                            {{ item.segments | length -1}} {{ 'flights.stop' | trans }}s
                                        {% endif %}
                                    </p>
                                </div>
                                {#<div class="hr" style="height:1px;border-bottom:1px solid #cccccc;clear: both;">&nbsp;</div>#}

                                    {% for segment in item.segments %}
