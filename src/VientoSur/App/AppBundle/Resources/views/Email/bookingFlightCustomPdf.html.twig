                                            <h7 style="margin: 0; padding: 0;text-align: center;display: block;">{{ 'flights.adult' | trans }} </h7>
                                            <h3 style="text-align: center;">{{ itineraryDetail.passenger_distribution.adults }}</h3>
                                        </div>
                                        {% if itineraryDetail.passenger_distribution.children > 0 %}
