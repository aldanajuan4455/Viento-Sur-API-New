<body style="margin:0; padding:0;" bgcolor="#F0F0F0" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
{% set targetCurrency = app.session.get('targetCurrency') %}
    {% if targetCurrency == 'USD' %}
        {% set symbol = 'US$' %}
    {% else %}
        {% set symbol = 'AR$' %}
    {% endif %}
    {% set decimal =false %}
    {#{{ dump(itineraryDetail) }}#}
    {#{{ dump(reservation) }}#}
    {#{{ dump(app.session.get('multipledestinations')) }}#}
    <!-- 100% background wrapper (grey background) -->
    <table border="0" width="100%" height="100%" cellpadding="0" cellspacing="0" bgcolor="#F0F0F0">
        <tr>
            <td align="center" valign="top" bgcolor="#F0F0F0" style="background-color: #F0F0F0;">

                <br>

                <!-- 600px container (white background) -->
                <table border="0" width="900" cellpadding="0" cellspacing="0" class="container" style="width:900px;max-width:900px">
                    <tr>
                        <td class="container-padding header" align="left" style="font-family:Helvetica, Arial, sans-serif;font-size:24px;font-weight:bold;padding-bottom:12px;color:#DF4726;padding-left:24px;padding-right:24px">

                        </td>
                    </tr>
                    <tr>
                        <td class="container-padding content" align="left" style="padding-left:24px;padding-right:24px;padding-top:12px;padding-bottom:12px;background-color:#ffffff">
                            <br>
                            <!--<div class="title" style="font-family:Helvetica, Arial, sans-serif;font-size:18px;font-weight:600;color:#374550">Two Columns (simple)</div>-->
                            <br>

                            <div class="body-text" style="background-color:#eee; font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333">
                                <p style="text-align: center;">{{ 'index.contact_us' | trans }} +54 351 4877555 - 4877444</p>
                            </div>

                            <div class="hr" style="height:1px;border-bottom:1px solid #cccccc">&nbsp;</div>
                            <br>

                            <!-- example: two columns (simple) -->

                            <!--[if mso]>
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                <tr><td width="50%" valign="top"><![endif]-->
                            <table width="264" border="0" cellpadding="0" cellspacing="0" align="left" class="force-row">
                                <tr>
                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333;width:100%">
                                        <img src="http://www.vientosur.net/bundles/vientosurappapp/images/vientosur-logo-color.png" alt="Viento Sur" title="">
                                    </td>
                                </tr>
                            </table>

                            <!--[if mso]></td><td width="50%" valign="top"><![endif]-->

                            <table width="264" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row">
                                <tr>
                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333;width:100%">
                                        {{ 'hotels.reservation_number' | trans }}
                                        <h3>{{ reservation.reservationId }}</h3>
                                    </td>
                                </tr>
                            </table>
                            {% if itineraryDetail.outbound is defined %}
                            <table width="264" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row">
                                <tr>
                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333;width:100%">
                                        {{ 'index.flight' | trans }}
                                        <h3>{{ app.session.get('origin_flight').text|split(',')[0] }}  -  {{ app.session.get('destination_flight').text|split(',')[0] }}</h3>
                                    </td>
                                </tr>
                            </table>
                            {% endif %}
                            <br>
                            <div class="hr" style="height:1px;border-bottom:1px solid #cccccc;clear: both;">&nbsp;</div>
                            <br><br>
                            <!--[if mso]></td></tr></table><![endif]-->
                            <div class="subtitle" style="font-family:Helvetica, Arial, sans-serif;font-size:16px;font-weight:600;color:#2469A0">
                                {#{{ 'hotels.thanks_booking' | trans({'%name%': 'phils'}) }}#}
                                {{ 'hotels.thanks_booking' | trans({'%name%': reservation.holderName}) }}
                            </div>

                            <div class="body-text" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:left;color:#333333">
                                {{ 'hotels.phone' | trans }}:
                                {{ reservation.phoneNumber }}
                                 <br>
                                Email:
                                {{ reservation.email }} <br>
                            </div>

                            <br>

                            <!--/ end example -->

                            <div class="hr" style="height:1px;border-bottom:1px solid #cccccc;clear: both;">&nbsp;</div>
                            <br>

                            <table width="350" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row">
                                <tr>
                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:center;color:#333333;width:100%">
                                        <div style="padding: 0 45px">
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'flights.rate_per_adult' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;"> {{ symbol }} {{ itineraryDetail.price_detail.adult_base |round| currency_convert_format(targetCurrency,decimal,false)| number_format(0, ',', '.')}}</p>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'flights.adult' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;"> {{ symbol }} {{ itineraryDetail.price_detail.adults_subtotal | round| currency_convert_format(targetCurrency,decimal, false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                            {% if itineraryDetail.passenger_distribution.children > 0 %}
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'index.search.hotel.childs' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;">{{ symbol }} {{ itineraryDetail.price_detail.children_subtotal | round| currency_convert_format(targetCurrency,decimal, false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                            {% endif %}
                                            {% if itineraryDetail.passenger_distribution.infants > 0 %}
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'flights.labels.infants' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;"> {{ symbol }} {{ itineraryDetail.price_detail.infants_subtotal | round| currency_convert_format(targetCurrency,decimal, false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                            {% endif %}
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'hotels.taxes_and_rates' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;">{{ symbol }} {{ itineraryDetail.price_detail.taxes | round| currency_convert_format(targetCurrency,decimal, false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'flights.charges' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;">{{ symbol }} {{ itineraryDetail.price_detail.fees | round| currency_convert_format(targetCurrency,decimal,false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: left;">
                                                <strong>{{ 'hotels.total_price' | trans }}</strong>
                                            </div>
                                            <div style="width: 48%; display: inline-block;text-align: right;">
                                                <p style="margin: 0; padding: 0;">{{ symbol }} {{ itineraryDetail.price_detail.total | round| currency_convert_format(targetCurrency,decimal,false)| number_format(0, ',', '.') }}</p>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </table>

