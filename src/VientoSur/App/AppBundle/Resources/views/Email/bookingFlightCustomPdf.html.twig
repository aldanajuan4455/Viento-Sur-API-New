                                <tr>
                                    <td class="col" valign="top" style="font-family:Helvetica, Arial, sans-serif;font-size:14px;line-height:20px;text-align:center;color:#333333;width:100%">
                                        {{ 'flights.return' | trans }}
                                        <h3>{{ itineraryDetail.inbound_choices[0].segments[0].departure_datetime | localizeddate('long', 'none', app.request.locale) }}</h3>
                                    </td>
                                </tr>
                            </table>
                            {% endif %}
                            {% if itineraryDetail.inbound_choices is defined %}
