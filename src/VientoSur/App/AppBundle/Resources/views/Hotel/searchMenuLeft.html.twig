                            <label for="childrenRoom3">{{ 'index.search.hotel.childs' | trans }}</label>
                            <select id="childrenRoom3" class="form-control coupled" data-couple="adults3.0" name="childrenRoomSelector3">
                                {% for i in 1..8 %}
                                    <option value="{{ loop.index0 }}" {% if data_travelers[3].children is defined and data_travelers[3].children | length == loop.index0 %}selected="selected"{% endif %}>{{ loop.index0 }}</option>
