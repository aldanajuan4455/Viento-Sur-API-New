{% set data_travelers = app.session.get('data_travelers') %}
<div class="content-sidebar">
    <form class="booking-item-dates-change mb5" name="searchHotel" id="search-hotels" action ="{{ path('viento_sur_process_search_hotels',  {'_locale': app.request.locale})}}" method="POST">
        <div class="form-group form-group-icon-left form-icon-lg"><i class="fa fa-map-marker input-icon input-icon-hightlight"></i>
            <label class="mfont">{{ 'hotels.search_menu.destiny' | trans }}</label>
            <!--colocar valor por defecto que trae la busqueda inicial-->
            <input id="autocomplete" name="autocomplete" type="text" class="city-input form-control" data-provide="typeahead" placeholder="Destino, CÃ³digo Postal" value="{% if app.session.get('destination').text is defined %} {{ app.session.get('destination').text }} {% endif %}" />
            <input id="destinationCity" name="cityInput" type="hidden" value="{% if app.session.get('destination').text is defined %}{{ app.session.get('destination').id }} {% endif %}" />
        </div>
        <div > <!--class="input-daterange" data-date-format="d M, D"-->
            <div class="form-group form-group-icon-left form-icon-lg"><i class="fa fa-calendar input-icon input-icon-hightlight"></i>
                <label class="sfont">{{ 'index.search.hotel.checkin_date' | trans }}</label>
                <input id="field-hotel-checkin" class="form-control required" name="start" type="text" required="" placeholder="dd/mm/yyyy" value="{{app.session.get('checkin_date')}}"/>
            </div>
            <div class="form-group form-group-icon-left form-icon-lg"><i class="fa fa-calendar input-icon input-icon-hightlight"></i>
                <label class="sfont">{{ 'index.search.hotel.checkout_date' | trans }}</label>
                <input id="field-hotel-checkout" class="form-control required" name="end" type="text" required="" placeholder="dd/mm/yyyy" value="{{app.session.get('checkout_date')}}"/>
            </div>
        </div>

        <div class="form-group">
            <label for="roomsQty">{{ 'index.search.hotel.rooms' | trans }}</label>
            <select id="roomsQty" class="form-control" name="habitacionesCant">
                <option value="1" {% if data_travelers| length == 1 %}selected{% endif %}>1 {{ 'index.search.hotel.room' | trans }}</option>
                <option value="2" {% if data_travelers| length == 2 %}selected{% endif %}>2 {{ 'index.search.hotel.rooms' | trans }}</option>
                <option value="3" {% if data_travelers| length == 3 %}selected{% endif %}>3 {{ 'index.search.hotel.rooms' | trans }}</option>
                <option value="4" {% if data_travelers| length == 4 %}selected{% endif %}>4 {{ 'index.search.hotel.rooms' | trans }}</option>
            </select>
        </div>

        <div id="room" {% if data_travelers | length > 1 %}style="display: none;"{% endif %}>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="adults">{{ 'index.search.hotel.adults' | trans }}</label>
                        <input type="hidden" value="0" id="adultsRoom">
                        <select id="adults" class="form-control coupled" data-couple="childrenRoom.0" name="adultsSelector">
                            {% for i in 1..8 %}
                                <option value="{{ loop.index }}" {% if data_travelers != null and data_travelers[1].adults == loop.index %}selected="selected"{% endif %}>{{ loop.index }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="childrenRoom">{{ 'index.search.hotel.childs' | trans }}</label>
                        <select id="childrenRoom" class="form-control coupled" data-couple="adults.1" name="childrenRoomSelector">
                            {% for i in 0..7 %}
                                <option value="{{ loop.index0 }}" {% if data_travelers[1].children is defined and data_travelers[1].children | length == loop.index0 %}selected="selected"{% endif %}>{{ loop.index0 }}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="clearBox"></div>
                </div>
            </div>
            <div class="hide" id="ageBlock">
                    <label id="lblAge">{{ 'index.search.hotel.childs_ages' | trans }}</label>
                    <div class="row">
                        {% for i in 1..7 %}
                            <div class="col-md-6 childAge{{ loop.index }}room {% if data_travelers[1].children is defined and  data_travelers[1].children| length < loop.index %}hide{% endif %}" style="margin-bottom:6px;">
                                <select id="childAge{{ loop.index }}room" class="form-control" name="childAgeSelector-{{ loop.index }}">
                                    <option value="" selected="selected">{{ 'index.search.hotel.age' | trans }}</option>
                                    {% for j in 1..18 %}
                                        <option value="{{ loop.index0 }}" {% if data_travelers[1].children[i] is defined and  data_travelers[1].children[i] == loop.index0 %}selected="selected"{% endif %}>{{ loop.index0 }}</option>
                                    {% endfor %}
                                </select>
                            </div>
                        {% endfor %}
                    </div>
                    <div class="clearBox"></div>
                </div>
        </div>
        <div class="row  {% if data_travelers| length < 2  %}hide{% endif %}" id="extraRooms">
            <div class="room1 {% if data_travelers| length < 1 %}hide{% endif %}" style="background-color:#f5f5f5">
                <div class="row">
                    <div class="col-md-12"><div class="form-group"><label class="room-label roomLabel">{{ 'index.search.hotel.room' | trans }} 1</label></div></div>
                </div>
