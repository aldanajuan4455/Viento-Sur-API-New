                <div class="row">
                    <div class="col-md-12"><div class="form-group"><label class="room-label roomLabel">{{ 'index.search.hotel.room' | trans }} 3</label></div></div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="adults3">{{ 'index.search.hotel.adults' | trans }}</label>
                            <input type="hidden" value="0" id="adultsRoom3">
                            <select id="adults3" class="form-control coupled" data-couple="childrenRoom3.0" name="adultsSelector3">
                                {% for i in 1..8 %}
                                    <option value="{{ i }}" {% if data_travelers[3].adults is defined and data_travelers[3].adults == i %}selected="selected"{% endif %}>{{ i }}</option>
                                {% endfor %}
