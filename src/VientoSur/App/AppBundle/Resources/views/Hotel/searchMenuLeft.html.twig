                    <div class="row">
                        {% for i in 1..7 %}
                            <div class="col-md-6 childAge{{ i }}2 child2" style="margin-bottom:6px;">
                                {#<div class="col-md-6 childAge11" style="margin-bottom:6px;">#}

                                {#<select id="childAge12" class="sb-age form-control required" name="childAgeSelector-2-1">#}
